.container{
    background-color: rgba(50, 186, 120, 0.5);
    height: 130rpx;
    width: 260rpx;
    position: fixed;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    margin-left: auto;
    margin-right: auto;
    border-radius: 130rpx 130rpx 0 0;
    text-align: center;
    box-sizing: border-box;
}
.container_ani{
    background-color: rgba(50, 186, 120, 0.3);
    height: 180rpx;
    width: 360rpx;
    position: fixed;
    left: 0rpx;
    right: 0rpx;
    bottom: -20rpx;
    margin-left: auto;
    margin-right: auto;
    border-radius: 180rpx 180rpx 0 0;
    animation: one-reclaim 0.6s infinite linear;
    animation-direction: alternate;
    text-align: center;
    box-sizing: border-box;

}
.btn{
    box-sizing: border-box;
    height: 110rpx;
    width: 220rpx !important;
    position: fixed;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    margin-left: auto !important;
    margin-right: auto !important;
    border-radius: 110rpx 110rpx 0 0;
    background-color: rgba(50, 186, 120, 1);
    color: white;
    border: 0rpx;
    align-items: center;
    justify-content: center;
    padding: 0 30rpx;
    font-size: 28rpx;
    line-height: 1;
    text-align: center;
    text-decoration: none;
    overflow: visible;
    transform: translate(0rpx, 0rpx);
}
.image{
    width: 15px;
    height: 15px;
    margin-top: 20rpx;
    justify-content: center;
    max-width: 100%;
    display: inline-block;
    position: relative;
    z-index: 0;
}
.appointment{
    margin-top: 10rpx;
    color: white;
}
@keyframes one-reclaim {
    0% {
        opacity: 0;
        transform: scale(0.5)
    }
    100% {
        opacity: 1;
        transform: scale(1)
    }
}